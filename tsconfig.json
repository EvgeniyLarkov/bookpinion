{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2019",
      "es2020.promise",
      "es2020.bigint",
      "es2020.string",
    ],
    "module": "commonjs",
    "target": "es2019",
    "jsx": "react",

    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "typeRoots": ["./node_modules/@types"]
}
